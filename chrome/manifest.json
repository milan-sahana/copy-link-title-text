{
  "manifest_version": 3,
  "name": "Copy Link Title",
  "version": "1.0",
  "description": "Right-click a link to copy its title attribute.",
  "permissions": ["contextMenus", "scripting", "clipboardWrite"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }
}
